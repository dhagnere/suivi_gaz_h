<%- include('./partials/header'); -%>
<%- include('./partials/navtop'); -%>
<%- include('./partials/navbar'); -%>

   <!-- Page Content -->
   <div id="page-content-wrapper">
    <button type="button" class="hamburger animated fadeInLeft is-closed" data-toggle="offcanvas">
        <span class="hamb-top"></span>
        <span class="hamb-middle"></span>
        <span class="hamb-bottom"></span>
    </button>
    
    <!-- Page Content -->
    <div class="container">
    
      <!-- Portfolio Item Heading -->
      <h1 class="my-4"><%=building.batiment%></h1>
    
      <!-- Portfolio Item Row -->
      <div class="row">
    
        <div class="col-md-8">
          <iframe src="<%building.maps%>" width="600" height="450" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>   
        </div>
    
        <div class="col-md-4">
          <h3 class="my-3">Adresse de l'entité :</h3>
          <p><%=building.adresse%></p>
          <p><%=building.zip%> <%=building.town%> </p>
    
          <h3 class="my-3">Détails techniques</h3>
          <ul>
            <li>Catégorie : <%=building.category%></li>
            <li>Numéro de pce : <%=building.pce%></li>
            <li>Fluides : <%=building.energy%></li>
            
          </ul>
        </div>

        <h3 class="my-3">Description : <%=building.description%></h3>

    
      </div>
      <!-- /.row -->
    
      <!-- Related Projects Row -->
      <h3 class="my-4">Related Projects</h3>
    
      <div class="row">
    
        <div class="col-md-3 col-sm-6 mb-4">
          <a href="#">
                <img class="img-fluid" src="http://placehold.it/500x300" alt="">
              </a>
        </div>
    
        <div class="col-md-3 col-sm-6 mb-4">
          <a href="#">
                <img class="img-fluid" src="http://placehold.it/500x300" alt="">
              </a>
        </div>
    
        <div class="col-md-3 col-sm-6 mb-4">
          <a href="#">
                <img class="img-fluid" src="http://placehold.it/500x300" alt="">
              </a>
        </div>
    
        <div class="col-md-3 col-sm-6 mb-4">
          <a href="#">
                <img class="img-fluid" src="http://placehold.it/500x300" alt="">
              </a>
        </div>
    
      </div>
      <!-- /.row -->
    
    </div>
    <!-- /.container -->
<!-- /#page-content-wrapper -->
</div>
<!-- /#wrapper -->



<%- include('./partials/footer'); -%>